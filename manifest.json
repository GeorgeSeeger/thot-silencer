{
  "manifest_version": 2,
  "name": "Thot Silencer",
  "version": "0.6",
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background":{
    "scripts": [ "background.js"]
  },
  "permissions": ["storage"]
}
