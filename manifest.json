{
  "manifest_version": 2,
  "name": "Thot Silencer",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png"},
  "permissions": ["storage"]
}
